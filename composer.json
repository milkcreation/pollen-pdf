{
  "name": "pollen-solutions/pdf",
  "description": "Pollen Pdf Component - Reading and writing PDF tools.",
  "homepage": "https://github.com/milkcreation/pollen-pdf.git",
  "keywords": [
    "pollen-solutions",
    "component",
    "pdf",
    "reading",
    "writing",
    "html converter"
  ],
  "time": "2021-09-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "dompdf/dompdf": "^1.0.2",
    "pollen-solutions/partial": "^1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Pdf\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Pdf\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container.",
    "pollen-solutions/routing" : "Pollen Routing Component - HTTP request mapping and HTTP response resolution."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
